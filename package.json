{
  "name": "agora-vue-ui",
  "version": "1.0.0",
  "description": "agora vue ui",
  "author": "houdong@agora.io",
  "private": false,
  "keywords": [
    "agora",
    "vue",
    "ui"
  ],
  "homepage": "https://github.com/Johnson-hd/agora-vue-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Johnson-hd/agora-vue-ui.git"
  },
  "bugs": {
    "url": "https://github.com/Johnson-hd/agora-vue-ui/issues"
  },
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "build:common": "webpack --inline --config build/webpack.build.common.conf.js",
    "build:component": "webpack --inline --config build/webpack.build.component.conf.js",
    "build:css": "webpack --inline --config build/webpack.build.css.conf.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "agora-md2vue-loader": "^1.0.1",
    "css-loader": "^3.5.3",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "fork-ts-checker-webpack-plugin": "^4.1.6",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.7",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-custom-properties": "^9.1.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "url-loader": "^4.1.0",
    "vue-loader": "^15.9.2",
    "vue-property-decorator": "^8.4.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "vue": "^2.6.11",
    "vue-router": "^3.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{vue,ts,tsx,js}": [
      "eslint --fix"
    ],
    "*.{css,scss}": [
      "prettier --parser=css --write"
    ]
  },
  "license": "MIT"
}
